if test -n "$withval"; then
  kanji=1
fi

  if test $kanji = 1; then
    if test -r ../tk3.6-jp/tk.h; then
      KANJI_NAME="-jp"
      TK_SOURCE="../tk3.6-jp"
      TK_VERSION="3.6"
    elif test -r ../tk3.6jp/tk.h; then
      KANJI_NAME="-jp"
      TK_SOURCE="../tk3.6jp"
      TK_VERSION="3.6"
    else
