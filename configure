  TCL_SOURCE="../tcl7.3"
  TCL_VERSION="7.3"
elif test -r ../tcl7.3-jp/tcl.h; then
  TCL_SOURCE="../tcl7.3-jp"
  TCL_VERSION="7.3"
elif test -r ../tcl7.3jp/tcl.h; then
  TCL_SOURCE="../tcl7.3jp"
  TCL_VERSION="7.3"
